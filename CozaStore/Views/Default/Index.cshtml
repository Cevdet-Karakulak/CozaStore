@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
@await Component.InvokeAsync("_DefaultHeadComponent")
<body class="animsition">

    @await Component.InvokeAsync("_DefaultNavbarComponent")

    @await Component.InvokeAsync("_DefaultSliderComponent")

    @await Component.InvokeAsync("_DefaultCategoryComponent") 

    @await Component.InvokeAsync("_DefaultProductComponent")

    @await Component.InvokeAsync("_DefaultFooterComponent")

    <!-- Back to top -->
    <div class="btn-back-to-top" id="myBtn">
        <span class="symbol-btn-back-to-top">
            <i class="zmdi zmdi-chevron-up"></i>
        </span>
    </div>

  

    @await Component.InvokeAsync("_DefaultScriptComponent")

</body>
</html>
